{
  "name": "fc-code-test",
  "version": "1.0.0",
  "description": "Code Test",
  "scripts": {
    "start": "npm run start:client & npm run start:server",
    "start:client": "parcel watch --no-hmr app/client/public/index.html",
    "start:server": "nodemon app/server",
    "build": "parcel build app/public/index.html --public-url ./"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/underscoredotspace/fc-code-test.git"
  },
  "author": "Colin Tindle <ampersand@underscore.space>",
  "license": "CC-BY-4.0",
  "devDependencies": {
    "babel-preset-stage-2": "^6.24.1",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.3",
    "parcel": "^1.9.7"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "mongodb": "^3.1.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "socket.io": "^2.1.1",
    "xml2js": "^0.4.19"
  },
  "babel": {
    "presets": [
      "stage-2"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  }
}
